{
  "extends": "@tsconfig/vite-react/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
